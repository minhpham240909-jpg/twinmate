{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "crons": [
    {
      "path": "/api/study-sessions/cleanup",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/cleanup-presence",
      "schedule": "0 1 * * *"
    },
    {
      "path": "/api/cron/warmup",
      "schedule": "0 4 * * *"
    },
    {
      "path": "/api/cron/cleanup-analytics",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/aggregate-stats",
      "schedule": "0 3 * * *"
    }
  ]
}
